{
    "name": "rlzdesenv/validador-cpf-cnpj",
    "description": "Classe em PHP para validação de CPF e CNPJ.",
    "keywords": [
        "cnpj",
        "cpf",
        "validador",
        "documento",
        "brasil",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/rlzdesenv/validador-cpf-cnpj",
    "authors": [
        {
            "name": "Elvis Reis",
            "email": "elvisdosreis@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0||^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "RLZ\\ValidadorCpfCnpj\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RLZ\\ValidadorCpfCnpj\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "version": "1.2.2"
}
